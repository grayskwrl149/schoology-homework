{
  "version": "0.3.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "migrate": "node-pg-migrate",
    "start": "tsdx watch --target node",
    "build": "tsdx build",
    "dev": "tsdx build && node ./dist/index.js",
    "test": "tsdx test",
    "lint": "tsdx lint --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "name": "api-starter",
  "author": "Jerry Huckins",
  "module": "dist/api-starter.esm.js",
  "size-limit": [
    {
      "path": "dist/api-starter.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/api-starter.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "husky": "^4.3.5",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3",
    "@types/express": "^4.17.9",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/node-pg-migrate": "^2.3.1",
    "@types/pg": "^7.14.7"
  },
  "dependencies": {
    "express": "^4.17.1",
    "node-pg-migrate": "^5.9.0",
    "pg": "^8.5.1",
    "swagger-ui-express": "^4.1.5"
  }
}
